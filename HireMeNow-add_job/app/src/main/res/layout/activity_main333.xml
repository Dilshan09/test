<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity333">


    <Button
        android:id="@+id/helpcen"
        android:layout_width="370dp"
        android:layout_height="80dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="48dp"
        android:background="@drawable/border"
        android:backgroundTint="@color/material_dynamic_neutral90"
        android:drawableLeft="@drawable/report_logo"
        android:drawableRight="@drawable/arrow_forward"
        android:fontFamily="@font/baloo_bhaina_2_semibold"
        android:gravity="center"
        android:text="Parking Report"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/con"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/con"
        android:layout_width="371dp"
        android:layout_height="80dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="48dp"
        android:background="@drawable/border"
        android:backgroundTint="@color/material_dynamic_neutral90"
        android:drawableLeft="@drawable/parking_his_logo"
        android:drawableRight="@drawable/arrow_forward"
        android:fontFamily="@font/baloo_bhaina_2_semibold"
        android:gravity="center"
        android:text="Parking  history"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/applyJob"
        android:layout_width="377dp"
        android:layout_height="80dp"
        android:layout_marginStart="48dp"
        android:layout_marginEnd="48dp"
        android:background="@drawable/border"
        android:backgroundTint="@color/material_dynamic_neutral90"
        android:drawableLeft="@drawable/logout_logo"
        android:drawableRight="@drawable/arrow_forward"
        android:fontFamily="@font/baloo_bhaina_2_semibold"
        android:gravity="center"
        android:text="Log Out                  "
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.435"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/helpcen"
        app:layout_constraintVertical_bias="0.321"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/profile"
        android:layout_width="366dp"
        android:layout_height="80dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="48dp"
        android:background="@drawable/border"
        android:backgroundTint="@color/material_dynamic_neutral90"
        android:drawableLeft="@drawable/parking_logo"
        android:drawableRight="@drawable/arrow_forward"
        android:fontFamily="@font/baloo_bhaina_2_semibold"
        android:gravity="center"
        android:text="Add Parking             "
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.627"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile2"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/profile2"
        android:layout_width="416dp"
        android:layout_height="200dp"
        android:layout_marginStart="48dp"
        android:layout_marginEnd="48dp"
        android:background="@drawable/border"
        android:backgroundTint="@android:color/holo_orange_light"
        android:fontFamily="@font/acme"
        android:gravity="center"
        android:text="Property Manager"
        android:textColor="@color/black"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>